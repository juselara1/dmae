{
    "path": "./data/",
    "n_samples": 70000,
    "pretrain_batch": 256,
    "cluster_batch": 256,
    "num_parallel_reads": 4,
    "dataset_name": "mnist",
    "input_shape": [28, 28, 1],
    "augmentation": {
	"random_rotation": {
	    "minval": -0.3, 
	    "maxval": 0.3
	},
	"width_shift_range": {
	    "minval": -2,
	    "maxval": 2
	},
	"height_shift_range": {
	    "minval": -2,
	    "maxval": 2
	},
	"horizontal_flip": false,
	"vertical_flip": false
    },
    "augment_autoencoder": true,
    "augment_clustering": false
}
